{
  "name": "solved-api",
  "version": "1.0.0",
  "description": "Initial project",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r tsconfig-paths/register src/server.ts",
    "migration:create": "typeorm migration:create --name",
    "migration:generate": "ts-node-dev -r dotenv/config ./node_modules/typeorm/cli.js migration:generate --name",
    "migration:revert": "ts-node-dev -r dotenv/config ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node-dev -r dotenv/config ./node_modules/typeorm/cli.js migration:run",
    "postgres:start": "docker run -d -p 5432:5432 --name postgres -e POSTGRES_HOST_AUTH_METHOD=trust postgres:13-alpine",
    "database:create": "docker exec postgres psql -U postgres postgres -c \"CREATE DATABASE $(grep DATABASE_NAME .env | cut -d '=' -f 2)\"",
    "eslint:check": "eslint '**/*ts'",
    "eslint:fix": "eslint --fix '**/*.ts'",
    "test": "jest"
  },
  "author": "Erick Kokubum",
  "license": "ISC",
  "dependencies": {
    "@fnando/cpf": "^1.0.2",
    "@sendgrid/mail": "^7.4.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.1.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "typeorm-naming-strategies": "^2.0.0",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@sendgrid/client": "^7.4.2",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.1",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "npm": "^7.8.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0"
  }
}
